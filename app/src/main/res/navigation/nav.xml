<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav"
    app:startDestination="@id/profileFragment2">
    <fragment
        android:id="@+id/profileFragment2"
        android:name="com.example.yeapp.Activity.ProfileFragment"
        android:label="fragment_profile"
        tools:layout="@layout/fragment_profile" >
        <action
            android:id="@+id/action_profileFragment2_to_profileFragmentImg"
            app:destination="@id/profileFragmentImg" />
    </fragment>
    <fragment
        android:id="@+id/profileFragmentImg"
        android:name="com.example.yeapp.Activity.ProfileFragmentImg"
        android:label="fragment_profile_img"
        tools:layout="@layout/fragment_profile_img" >
        <action
            android:id="@+id/action_profileFragmentImg_to_mainActivity"
            app:destination="@id/mainActivity" />
    </fragment>
    <activity
        android:id="@+id/mainActivity"
        android:name="com.example.yeapp.Activity.MainActivity"
        android:label="activity_main"
        tools:layout="@layout/activity_main" />
</navigation>